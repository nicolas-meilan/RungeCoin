{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "isolatedModules": false,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "es6",
      "es2016.array.include",
      "es2017.object",
      "es2019",
      "ES2021.String",
    ],
    "paths": {
      "@screens": [
        "./src/screens"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@components": [
        "./src/components"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@containers": [
        "./src/containers"
      ],
      "@containers/*": [
        "./src/containers/*"
      ],
      "@hooks": [
        "./src/hooks"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@navigation": [
        "./src/navigation"
      ],
      "@navigation/*": [
        "./src/navigation/*"
      ],
      "@system": [
        "./src/system"
      ],
      "@system/*": [
        "./src/system/*"
      ],
      "@http": [
        "./src/http"
      ],
      "@http/*": [
        "./src/http/*"
      ],
      "@web3": [
        "./src/web3"
      ],
      "@web3/*": [
        "./src/web3/*"
      ],
      "@utils": [
        "./src/utils"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@assets": [
        "./assets"
      ],
      "@assets/*": [
        "./assets/*"
      ],
    },
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": ["styled-components-react-native"],
    "typeRoots": [
      "node_modules/@types",
      "src/@types"
    ],
  },
}
