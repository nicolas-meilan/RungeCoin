{
  "name": "rungecoin",
  "version": "1.5.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint --quiet --no-error-on-unmatched-pattern . --ext .ts,.tsx --cache",
    "lint-fix": "npm run lint -- --fix",
    "typescript": "tsc --project tsconfig.json",
    "start": "react-native start --resetCache",
    "test": "jest",
    "build-android-development": "eval $(./scripts/parsedEnv.sh) && cd android && ./gradlew bundleDebug && cd ..",
    "build-android-release-candidate": "eval $(./scripts/parsedEnv.sh) && cd android && ./gradlew bundleDebug && cd ..",
    "build-android-release": "eval $(./scripts/parsedEnv.sh) && cd android && ./gradlew bundleRelease && cd ..",
    "distribute-development-flow": "eval $(./scripts/parsedEnv.sh) && ./scripts/upload-build-development-flow.sh",
    "distribute-production-flow": "eval $(./scripts/parsedEnv.sh) && ./scripts/upload-build-production-flow.sh"
  },
